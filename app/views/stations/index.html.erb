<h1>Stations</h1>

<table class="table w-50">
  <thead>
    <tr>
      <th>Address</th>
      <th>Available Bikes</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @stations.each do |station| %>
      <% cache station do %>
        <%= render station %>
      <% end %>
    <% end %>
  </tbody>
</table>
